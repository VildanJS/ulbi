  "main": "index.js",
  "scripts": {
    "start:server": "node ./server/index.js",
    "start:front": "npx webpack serve --env mode=development port=3000  --hot ",
    "start:vite": "vite",
    "kill": "taskkill /f /im node.exe",
    "permisions": "Set-ExecutionPolicy -Scope CurrentUser RemoteSigned",
    "start": "concurrently \"npm run start:server\" \"npm run start:front\"",
    "lint:ts": "npx eslint \"**/*.{ts,tsx}\" ",
    "prettier": "npx prettier --write \"**/*.{ts, tsx, json}\"",
    "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\" --fix",
    "build:prod": "npx webpack --env mode=production apiUrl=http://34.16.139.145:8443",
    "build:dev": "npx webpack --env mode=development",
    "test:unit": "jest --config ./config/jest/jest.config.ts",
    "test:e2e": "cypress open",
    "test:ui": "loki test",
    "test:ui:update": "loki update",
    "test:ui:ok": "loki approve",
    "test:ui:ci": "concurrently --kill-others --raw --success first \"npm run storybook\" \"npm run test:ui\" ",
    "storybook": "storybook dev -p 6006 -c ./config/storybook",
    "storybook:build": "storybook build -c ./config/storybook",
    "visual:report": "npm run visual:report:json && npm run visual:report:html",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
  },
